Arduino to 16x2 LCD Display using Proteus
#include <LiquidCrystal.h>
LiquidCrystal lcd (7, 8, 9, 10, 11, 12);
// your PWM numbers may be different

void setup() {

lcd.begin(16, 2);

lcd.print("put your message here");

}

void loop () {

for (int i=0; 1<13; i++)
}

lcd.scrollDisplayLeft();
delay(700);

}
}
